<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.fs.expense.model.FsExpenseMainHang"
		table="fs_expense_main_hang">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdOffsetMoney"
			column="fd_offset_money"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdOverplusMoney"
			column="fd_overplus_money"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="true" />

		<many-to-one
			name="fdHangExpense"
			column="fd_hang_expense"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdOffsetExpense"
			column="fd_offset_expense"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
